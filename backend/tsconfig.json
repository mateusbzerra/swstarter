{
  "compilerOptions": {
    "baseUrl": "",
    "paths": {
      "@/*": ["src/*"]
    },
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  }
}
